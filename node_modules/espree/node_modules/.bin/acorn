#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/acorn/7.1.1/node_modules/acorn/bin/acorn/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/acorn/7.1.1/node_modules/acorn/bin/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/acorn/7.1.1/node_modules/acorn/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/acorn/7.1.1/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/acorn/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/node_modules:/mnt/c/Users/Fluffy Ball/Documents/node_modules:/mnt/c/Users/Fluffy Ball/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../acorn/7.1.1/node_modules/acorn/bin/acorn" "$@"
else 
  exec node  "$basedir/../../../../../../acorn/7.1.1/node_modules/acorn/bin/acorn" "$@"
fi
