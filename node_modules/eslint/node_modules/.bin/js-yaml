#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/js-yaml/3.13.1/node_modules/js-yaml/bin/js-yaml.js/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/js-yaml/3.13.1/node_modules/js-yaml/bin/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/js-yaml/3.13.1/node_modules/js-yaml/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/js-yaml/3.13.1/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/js-yaml/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/node_modules:/mnt/c/Users/Fluffy Ball/Documents/node_modules:/mnt/c/Users/Fluffy Ball/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../js-yaml/3.13.1/node_modules/js-yaml/bin/js-yaml.js" "$@"
else 
  exec node  "$basedir/../../../../../../js-yaml/3.13.1/node_modules/js-yaml/bin/js-yaml.js" "$@"
fi
