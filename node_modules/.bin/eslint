#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/eslint/6.8.0/node_modules/eslint/bin/eslint.js/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/eslint/6.8.0/node_modules/eslint/bin/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/eslint/6.8.0/node_modules/eslint/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/eslint/6.8.0/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/eslint/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/node_modules:/mnt/c/Users/Fluffy Ball/Documents/node_modules:/mnt/c/Users/Fluffy Ball/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/eslint/bin/eslint.js/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/eslint/bin/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/eslint/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint/bin/eslint.js" "$@"
else 
  exec node  "$basedir/../eslint/bin/eslint.js" "$@"
fi
