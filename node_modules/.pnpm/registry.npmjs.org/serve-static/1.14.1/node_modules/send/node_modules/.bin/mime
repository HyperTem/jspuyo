#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules/mime/cli.js/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules/mime/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/mime/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules/.pnpm/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/jspuyo/node_modules:/mnt/c/Users/Fluffy Ball/Documents/github/node_modules:/mnt/c/Users/Fluffy Ball/Documents/node_modules:/mnt/c/Users/Fluffy Ball/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../mime/1.6.0/node_modules/mime/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../../mime/1.6.0/node_modules/mime/cli.js" "$@"
fi
